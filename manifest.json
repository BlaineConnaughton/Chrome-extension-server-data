{
  "manifest_version": 2,

  "name": "CyberReason data",
  "description": "This extension grabs server data",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Get CyberReason Server Data"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/",
    "cookies",
    "https://*.cybereason.net/",
    "http://cybereasonserverdata.appspot.com/"
  ]
}